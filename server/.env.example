# Port du serveur
PORT=5000

# Clé secrète pour JWT (si implémenté ultérieurement)
JWT_SECRET=your_jwt_secret_key

# Configuration MongoDB
MONGO_URI=mongodb://localhost:27017/excel-meta-ai-matcher

# Configuration API Meta Marketing
META_APP_ID=your_meta_app_id
META_APP_SECRET=your_meta_app_secret
META_ACCESS_TOKEN=your_meta_access_token
